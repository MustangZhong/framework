{
    "name": "notadd/framework",
    "description": "The Notadd Foundation Framework.",
    "keywords": ["notadd", "cms", "foundation", "framework"],
    "homepage": "https://notadd.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "twilroad",
            "email": "heshudong@ibenchu.com"
        }
